{
  "name": "connectfe-backend",
  "version": "1.0.0",
  "description": "Backend do sistema ConnectFé - Plataforma de conexão e engajamento religioso",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "config:test": "node test-config.js",
    "config:export": "node -e \"require('./config').exportConfig('./config-export.json', 'json')\"",
    "config:export-env": "node -e \"require('./config').exportConfig('./config-export.env', 'env')\"",
    "config:validate": "node -e \"const config = require('./config'); const validation = config.validateSystemConfig(); console.log(JSON.stringify(validation, null, 2));\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "clean": "rm -rf node_modules package-lock.json",
    "install:clean": "npm run clean && npm install",
    "docker:build": "docker build -t connectfe-backend .",
    "docker:run": "docker run -p 3001:3001 connectfe-backend",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "health:check": "curl -f http://localhost:3001/health || exit 1",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:reset": "node scripts/reset-db.js",
    "backup:create": "node scripts/backup.js",
    "backup:restore": "node scripts/restore.js",
    "logs:view": "tail -f logs/app.log",
    "logs:clear": "> logs/app.log",
    "monitor:start": "node scripts/monitor.js",
    "monitor:stop": "pkill -f 'monitor.js'",
    "performance:test": "artillery run tests/performance/load-test.yml",
    "security:scan": "npm audit",
    "security:fix": "npm audit fix",
    "update:deps": "npm update",
    "update:deps:major": "npx npm-check-updates -u",
    "docs:generate": "jsdoc -c jsdoc.json",
    "docs:serve": "jsdoc -c jsdoc.json --serve",
    "deploy:staging": "bash scripts/deploy-staging.sh",
    "deploy:production": "bash scripts/deploy-production.sh",
    "rollback": "bash scripts/rollback.sh"
  },
